{
    "name": "moneo/laravel-request-forwarder",
    "description": "Laravel Request Forwarder allows you to forward incoming requests to another addresses.",
    "keywords": [
        "moneo",
        "laravel",
        "laravel-request-forwarder"
    ],
    "homepage": "https://github.com/moneo/laravel-request-forwarder",
    "license": "MIT",
    "authors": [
        {
            "name": "Emre Dipi",
            "email": "mail@emredipi.com",
            "role": "Developer"
        },
        {
            "name": "Emir Karşıyakalı",
            "homepage": "https://github.com/emir",
            "role": "Maintainer"
        }
    ],
    "require": {
        "php": "^8.2",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/queue": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^8.0",
        "larastan/larastan": "^3.0",
        "orchestra/testbench": "^8.8|^9.0|^10.0",
        "pestphp/pest": "^2.20|^3.0",
        "pestphp/pest-plugin-arch": "^2.5|^3.0",
        "pestphp/pest-plugin-laravel": "^2.0|^3.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0|^2.0",
        "phpstan/phpstan-phpunit": "^1.0|^2.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Moneo\\RequestForwarder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Moneo\\RequestForwarder\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-laravel-request-forwarder --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build",
            "@php vendor/bin/testbench serve"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Moneo\\RequestForwarder\\RequestForwarderServiceProvider"
            ],
            "aliases": {
                "RequestForwarder": "Moneo\\RequestForwarder\\Facades\\RequestForwarder"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
